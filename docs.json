{
	"$schema": "https://mintlify.com/docs.json",
	"name": "Rewrite",
	"theme": "mint",
	"appearance": {
		"default": "dark",
		"strict": true
	},
	"errors": {
		"404": {
			"redirect": false,
			"title": "Rewrite - Not Found",
			"description": "Hey, where did the page go?"
		}
	},
	"colors": {
		"primary": "#9A9A9A",
		"light": "#9A9A9A",
		"dark": "#9A9A9A"
	},
	"fonts": {
		"heading": {
			"family": "Inter"
		},
		"body": {
			"family": "Lato"
		}
	},
	"favicon": "/favicon.svg",
	"logo": {
		"light": "/images/icon.png",
		"dark": "/images/icon.png"
	},
	"navbar": {
		"links": [
			{
				"label": "Dashboard",
				"href": "https://dashboard.rewritetoday.com"
			},
			{
				"label": "GitHub",
				"icon": {
					"name": "github"
				},
				"href": "https://github.com/rewritetoday"
			}
		]
	},
	"footer": {
		"socials": {
			"x": "https://x.com/rewritetoday",
			"github": "https://github.com/rewritetoday",
			"discord": "https://discord.gg/rewrite"
		}
	},
	"api": {
		"mdx": {
			"server": "https://api.rewritetoday.com/v1",
			"auth": {
				"method": "bearer",
				"name": "Bearer"
			}
		}
	},
	"styling": {
		"codeblocks": {
			"theme": "vesper"
		}
	},
